{
  "dashboard": {
    "id": null,
    "title": "MLOps Model Performance Dashboard",
    "tags": ["mlops", "model-monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Inference Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(inference_requests_total[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "options": {
          "displayMode": "list",
          "orientation": "horizontal"
        }
      },
      {
        "id": 2,
        "title": "Total Inference Requests",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(inference_requests_total)",
            "legendFormat": "Total Requests"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 3,
        "title": "Request Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(inference_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(inference_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
        "options": {
          "displayMode": "list",
          "orientation": "horizontal"
        }
      },
      {
        "id": 4,
        "title": "Model Accuracy",
        "type": "singlestat",
        "targets": [
          {
            "expr": "model_accuracy_percentage",
            "legendFormat": "Accuracy %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 5,
        "title": "Prediction Confidence Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(prediction_confidence_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(prediction_confidence_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "options": {
          "displayMode": "list",
          "orientation": "horizontal"
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
